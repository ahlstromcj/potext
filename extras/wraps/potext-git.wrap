[wrap-git]

url = https://github.com/ahlstromcj/potext.git
revision = head
depth = 1

[provide]

#----------------------------------------------------------------------------
# These dependencies are declared in potext/library/meson.build.
#----------------------------------------------------------------------------

libpotext_include = libpotext_include_dep
libpotext_library = libpotext_library_dep

